{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": [
            "id",
            "firstname",
            "lastname",
            "email"
        ],
        "properties": {
            "id": {
                "type": "integer",
                "default": 0,
                "title": "The id Schema",
                "examples": [
                    1
                ]
            },
            "firstname": {
                "type": "string",
                "default": "",
                "title": "The firstname Schema",
                "examples": [
                    "Sam"
                ]
            },
            "lastname": {
                "type": "string",
                "default": "",
                "title": "The lastname Schema",
                "examples": [
                    "Patel"
                ]
            },
            "email": {
                "type": "string",
                "default": "",
                "title": "The email Schema",
                "examples": [
                    "sp@email.com"
                ]
            }
        },
        "examples": [{
            "id": 1,
            "firstname": "Sam",
            "lastname": "Patel",
            "email": "sp@email.com"
        }]
    },
    "examples": [
        [{
            "id": 1,
            "firstname": "Sam",
            "lastname": "Patel",
            "email": "sp@email.com"
        }]
    ]
}